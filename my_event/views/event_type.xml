<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="event_type_action" model="ir.actions.act_window">
        <field name="name">Events</field>
        <field name="res_model">event.type</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new Property, the first step of a new property!
            </p>
        </field>
    </record>

    <!--tree-->
    <record id="event_type_view_tree" model="ir.ui.view">
        <field name="name">event.org.view.tree</field>
        <field name="model">event.type</field>
        <field name="arch" type="xml">
            <tree string="treeeeeee">
                <field name="name" />
                <field name="venue" />
            </tree>
        </field>
    </record>

    <!--form-->
    <record id="event_type_view_form" model="ir.ui.view">
        <field name="name">event.type.view.tree</field>
        <field name="model">event.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="Start_date" />
                        <field name="end_date"/>
                        <field name="budget" />
                        <field name="venue" />
                        <field name="no_of_guest" />
                        <field name="event_ids"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>